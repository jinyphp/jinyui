{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^3.1",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jiny/polyglot": "^0.0.3",
        "jinyxe/ex-echo": "^0.0.2",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "webuni/front-matter": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Jiny\\Html\\": "jiny/html/src",
            "Jiny\\Theme\\": "jiny/theme/src",
            "Jiny\\Action\\": "jiny/action/src",
            "Jiny\\UI\\": "jiny/ui/src",
            "Jiny\\DB\\": "jiny/database/src",
            "Jiny\\Table\\": "jiny/table/src",
            "Jiny\\Site\\": "jiny/site/src",
            "Jiny\\Sales\\": "jiny/sales/src",
            "Jiny\\Pages\\": "jiny/pages/src",
            "Jiny\\Members\\": "jiny/members/src"
        },
        "files" : [
            "jiny/html/src/Helpers/Helper.php",
            "jiny/html/src/Helpers/Factory.php",
            "jiny/theme/src/Helpers/Helper.php",
            "jiny/action/src/Helpers/Helper.php",
            "jiny/ui/src/Helpers/Helper.php",
            "jiny/database/src/Helpers/Helper.php",
            "jiny/table/src/Helpers/Helper.php",
            "jiny/site/src/Helpers/Helper.php",
            "jiny/sales/src/Helpers/Helper.php",
            "jiny/pages/src/Helpers/Helper.php",
            "jiny/members/src/Helpers/Helper.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
